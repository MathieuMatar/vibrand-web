    <script>
        const headerLinks = Array.from(document.querySelectorAll('header a')).slice(1); // Skip the first link
        //get all elements of class project-links
        const projectLinks = Array.from(document.querySelectorAll('.project-links'));
        
        headerLinks.push(...projectLinks);


        headerLinks.forEach(link => {
            link.addEventListener('click', function (event) {
                event.preventDefault();
                const href = link.getAttribute('href');
                //history.pushState(null, '', href);

                fetch(href)
                    .then(response => response.text())
                    .then(html => {
                        const parser = new DOMParser();
                        const doc = parser.parseFromString(html, "text/html");
                        const newParent = doc.querySelector(".parent");
                        const currentParent = document.querySelector(".parent");

                        if (newParent && currentParent) {
                            currentParent.replaceWith(newParent);
                        }
                    })
                    .catch(error => {
                        console.error("Failed to load page:", error);
                    });
            });
        });
    </script>